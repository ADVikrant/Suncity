                                <option value="1">Single Member</option>
                                <option value="2">Couple</option>

                                <option value="3">Couple + 1 children</option>
                                <option value="4">Couple + 2 children</option>
                                <option value="5">Couple + 2 children + parents</option>
                            </select>
                        </div>
